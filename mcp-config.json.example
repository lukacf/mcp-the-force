{
  "mcpServers": {
    "second-brain": {
      "command": "uv",
      "args": ["run", "--", "mcp-second-brain"],
      "env": {
        "OPENAI_API_KEY": "sk-proj-YOUR_OPENAI_API_KEY_HERE",
        "VERTEX_PROJECT": "your-gcp-project-id",
        "VERTEX_LOCATION": "us-central1",
        "VERTEX_ACCOUNT": "your-email@your-domain.com",
        "HOST": "127.0.0.1",
        "PORT": "8000",
        "CONTEXT_PERCENTAGE": "0.9",
        "DEFAULT_TEMPERATURE": "0.2",
        "LOG_LEVEL": "INFO"
      },
      "timeout": 3600000,
      "description": "MCP Second-Brain server - AI model orchestrator with intelligent context management"
    }
  }
}